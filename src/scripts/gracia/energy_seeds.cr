  private enum GraciaSeed : UInt8
    INFINITY
    DESTRUCTION
    ANNIHILATION_BISTAKON
    ANNIHILATION_REPTILIKON
    ANNIHILATION_COKRAKON
  end

  def initialize
    super(self.class.simple_name, "gracia/AI")

    register_mobs(SEED_IDS)
    add_first_talk_id(SEED_IDS)
    add_first_talk_id(TEMPORARY_TELEPORTER)
    add_enter_zone_id(SOD_ZONE)
    start_ai
  end

  protected def self.seed_active?(seed)
    case seed
    when GraciaSeed::INFINITY
      return false
    when GraciaSeed::DESTRUCTION
      return GraciaSeedsManager.sod_state == 2
    when GraciaSeed::ANNIHILATION_BISTAKON..GraciaSeed::ANNIHILATION_COKRAKON
      return true
    end


    true
  end

  def on_skill_see(npc, caster, skill, targets, is_summon)
    if !targets.includes?(npc) || skill.id != 5780
      return super
    end

    npc.delete_me

    if sp = SPAWNS[SPAWNED_NPCS[npc]?]?
      sp.schedule_respawn(RESPAWN + Rnd.rand(RANDOM_RESPAWN_OFFSET))
      SPAWNED_NPCS.delete(npc)
      if EnergySeeds.seed_active?(sp.seed_id)
        item_id = 0

        case npc.id
        when 18678 # Water
          item_id = 14016
        when 18679 # Fire
          item_id = 14015
        when 18680 # Wind
          item_id = 14017
        when 18681 # Earth
          item_id = 14018
        when 18682 # Divinity
          item_id = 14020
        when 18683 # Darkness
          item_id = 14019
        else
          return super
        end
        if Rnd.rand(100) < 33
          caster.send_packet(SystemMessageId::THE_COLLECTION_HAS_SUCCEEDED)
          caster.add_item("EnergySeed", item_id, Rnd.rand(RATE + 1..2 * RATE).to_i64, nil, true)
        else
          caster.send_packet(SystemMessageId::THE_COLLECTION_HAS_SUCCEEDED)
          caster.add_item("EnergySeed", item_id, Rnd.rand(1..RATE).to_i64, nil, true)
        end
        seed_collect_event(caster, npc, sp.seed_id)
      end
    end

    super
  end

  def on_adv_event(event, npc, pc)
    if event.casecmp?("StartSoDAi")
      SEED_OF_DESTRUCTION_DOORS.each do |door_id|
        if door = DoorData.get_door(door_id)
          door.open_me
        end
      end
      start_ai(GraciaSeed::DESTRUCTION)
    elsif event.casecmp?("StopSoDAi")
      SEED_OF_DESTRUCTION_DOORS.each do |door_id|
        if door = DoorData.get_door(door_id)
          door.close_me
        end
      end
      ZoneManager.get_zone_by_id(SOD_ZONE).not_nil!.each_player_inside do |ch|
        ch.tele_to_location(SOD_EXIT_POINT)
      end
      stop_ai(GraciaSeed::DESTRUCTION)
    elsif event.casecmp?("DeSpawnTask")
      npc = npc.not_nil!
      if npc.in_combat?
        start_quest_timer("DeSpawnTask", 30000, npc, nil)
      else
        npc.delete_me
      end
    end

    nil
  end

  def on_first_talk(npc, pc)
    if npc.id == TEMPORARY_TELEPORTER
      pc.tele_to_location(SOD_EXIT_POINT)
    end
    pc.action_failed

    nil
  end

  def on_kill(npc, pc, is_summon)
    if sp = SPAWNS[SPAWNED_NPCS[npc]?]?
      sp.schedule_respawn(RESPAWN + Rnd.rand(RANDOM_RESPAWN_OFFSET))
      SPAWNED_NPCS.delete(npc)
    end

    super
  end

  def on_enter_zone(char, zone)
    if char.instance_id != 0
      return super
    end

    if char.is_a?(L2PcInstance)
      case zone.id
      when SOD_ZONE
        if !EnergySeeds.seed_active?(GraciaSeed::DESTRUCTION) && !char.gm?
          char.tele_to_location(SOD_EXIT_POINT)
        end
      end

    end

    super
  end

  def start_ai
    SPAWNS.each_value do |sp|
      if EnergySeeds.seed_active?(sp.seed_id)
        sp.schedule_respawn(0)
      end
    end
  end

  def start_ai(type : GraciaSeed)
    SPAWNS.each_value do |sp|
      if sp.seed_id == type
        sp.schedule_respawn(0)
      end
    end
  end

  def stop_ai(type : GraciaSeed)
    SPAWNED_NPCS.each_key do |seed|
      if type == SPAWNS[SPAWNED_NPCS[seed]].seed_id
        seed.delete_me
      end
    end
  end

  def seed_collect_event(pc, seed_energy : L2Npc, seed_type : GraciaSeed)
    return unless pc

    st = pc.get_quest_state(Q00692_HowtoOpposeEvil.simple_name)
    case seed_type
    when GraciaSeed::INFINITY
      if st && st.cond?(3)
        handle_quest_drop(pc, 13798)
      end
    when GraciaSeed::DESTRUCTION
      if st && st.cond?(3)
        handle_quest_drop(pc, 13867)
      end
    when GraciaSeed::ANNIHILATION_BISTAKON
      if st && st.cond?(3)
        handle_quest_drop(pc, 15535)
      end
      if Rnd.bool
        mob = spawn_suprise_mob(seed_energy, ANNIHILATION_SUPRISE_MOB_IDS[0].sample(random: Rnd))
        mob.set_running
        mob.add_damage_hate(pc, 0, 999)
        mob.set_intention(AI::ATTACK, pc)
      end
    when GraciaSeed::ANNIHILATION_REPTILIKON
      if st && st.cond?(3)
        handle_quest_drop(pc, 15535)
      end
      if Rnd.bool
        mob = spawn_suprise_mob(seed_energy, ANNIHILATION_SUPRISE_MOB_IDS[1].sample(random: Rnd))
        mob.set_running
        mob.add_damage_hate(pc, 0, 999)
        mob.set_intention(AI::ATTACK, pc)
      end
    when GraciaSeed::ANNIHILATION_COKRAKON
      if st && st.cond?(3)
        handle_quest_drop(pc, 15535)
      end
      if Rnd.bool
        mob = spawn_suprise_mob(seed_energy, ANNIHILATION_SUPRISE_MOB_IDS[2].sample(random: Rnd))
        mob.set_running
        mob.add_damage_hate(pc, 0, 999)
        mob.set_intention(AI::ATTACK, pc)
      end
    end

  end

  private def spawn_suprise_mob(energy, npc_id)
    template = NpcData[npc_id]
    monster = L2MonsterInstance.new(template)
    monster.heal!
    monster.heading = energy.heading
    monster.instance_id = energy.instance_id
    monster.show_summon_animation = true
    monster.spawn_me(energy.x, energy.y, energy.z)
    start_quest_timer("DeSpawnTask", 30000, monster, nil)
    monster
  end

  private def handle_quest_drop(pc, item_id)
    chance = HOW_TO_OPPOSE_EVIL_CHANCE * Config.rate_quest_drop
    num_items = (chance / 100).to_i
    chance = chance % 100
    if Rnd.rand(100) < chance
      num_items += 1
    end
    if num_items > 0
      give_items(pc, item_id, num_items)
      play_sound(pc, Sound::ITEMSOUND_QUEST_ITEMGET)
    end
  end

  private struct ESSpawn
    getter seed_id
    initializer spawn_id : Int32, seed_id : GraciaSeed, loc : Location,
      npc_ids : Array(Int32)

    def schedule_respawn(delay)
      task = -> do
        # if the AI is inactive, do not spawn the NPC
        if EnergySeeds.seed_active?(@seed_id)
          # get a random NPC that should spawn at this location
          sp = AbstractScript.add_spawn(@npc_ids.sample(random: Rnd), @loc, false, 0)
          SPAWNED_NPCS[sp] = @spawn_id
        end
      end

      ThreadPoolManager.schedule_general(task, delay)
    end
  end
